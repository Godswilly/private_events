<p id="notice"><%= notice %></p>

<p>
  <strong>Creator:</strong>
  <%= @event.user.username %><br/>
  <%= @event.description %>
</p>

<strong>Event Attendees:</strong><br/>
<% @event.attendees.each do |f| %>

    <%= f.username %> </br>
<% end %>

<%= button_to 'Register for the event', join_event_attendees_path(id: @event.id), method: :post %>



<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
